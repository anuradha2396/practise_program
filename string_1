#include <stdio.h>

int main()
{
    char ch[20];
    int i,count=0;
    printf("Enter a string");
    scanf("%s",&ch[i]);
    while(i<20)
    {
        if(ch[i]=='a'||ch[i]=='e'||ch[i]=='i'||ch[i]=='o'||ch[i]=='u')
        {
            count++;
        }
        i++;
    }
    printf("total number of vowels=%d",count);
}

