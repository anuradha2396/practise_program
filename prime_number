#include<stdio.h>

int main()
{
  int m,n,flag=0,i;
  printf("Enter a number");
  scanf("%d",&n);
  m=n%2;
  for(i=2;i<=m;i++)
  {
      if(n%2==0)
      {
          printf("Not a prime number");
          flag=1;
          break;
      }
  }
  if(flag==0)
  {
    printf("Number is prime");
    return 0;
  }
}

