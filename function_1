#include <stdio.h>
int comp(int x ,int y)
{
   if(x>y)
   {
       return x;
   }
   else
   {
       return y;
   }
}  
int main()
{
   int x,y,z;
   printf("Enter the value of x\n");
   scanf("%d",&x);
   printf("Enter the value of y\n");
   scanf("%d",&y);
   z= comp(x,y);
   printf("on comparing we get=%d",z);
}

