#include<stdio.h>

void swap (int a, int b);

int main ()
{
  int m, n;
  printf ("Enter the value of m ");
  scanf ("%d", &m);
  printf("Enter the value of n ");
  scanf("%d",&n);
  swap (m, n);
}

void
swap (int a, int b)
{
  int tmp;
  tmp = a;
  a = b;
  b = tmp;
  printf ("values after swap m = %d\n",a);
   printf ("values after swap n = %d\n",b);
}

